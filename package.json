{
  "name": "btch-downloader",
  "version": "6.0.8",
  "description": "A lightweight TypeScript/JavaScript library for downloading media from social media platforms",
  "main": "dist/index.js",
  "browser": "dist/browser/index.js",
  "unpkg": "dist/browser/index.min.js",
  "jsdelivr": "dist/browser/index.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "engine-requirements.js"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/browser/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./site": {
      "types": "./dist/Defaults/site.d.ts",
      "import": "./dist/Defaults/site.js",
      "require": "./dist/Defaults/site.js"
    },
    "./http": {
      "types": "./dist/Http/index.d.ts",
      "import": "./dist/Http/index.js",
      "require": "./dist/Http/index.js"
    },
    "./types": {
      "types": "./dist/Types/index.d.ts",
      "import": "./dist/Types/index.js",
      "require": "./dist/Types/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsup",
    "build:browser": "tsup",
    "pub": "npm run build && npm publish --access public",
    "test": "npm run build && ts-node test/Test.ts",
    "docs": "npx jsdoc -c jsdoc.conf.json",
    "preinstall": "node ./engine-requirements.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "AIO",
    "Instagram",
    "TikTok",
    "Facebook",
    "YouTube",
    "Twitter",
    "X",
    "MediaFire",
    "Capcut",
    "Gdrive",
    "Pinterest",
    "Douyin",
    "Xiaohongshu",
    "Rednote",
    "SnackVideo",
    "Cocofun",
    "media-downloader",
    "social-media",
    "browser",
    "typescript"
  ],
  "author": "Tio",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.9.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "clean-jsdoc-theme": "^4.3.0",
    "esbuild": "^0.25.11",
    "install": "^0.13.0",
    "jsdoc": "^4.0.5",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hostinger-bot/btch-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/hostinger-bot/btch-downloader/issues"
  },
  "homepage": "https://github.com/hostinger-bot/btch-downloader#readme",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "engines": {
    "node": ">=20.18.1"
  },
  "packageManager": "yarn@4.10.3"
}
